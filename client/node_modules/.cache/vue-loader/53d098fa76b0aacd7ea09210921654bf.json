onse => response.json())\n      .then(json => (this.serverInfo = json))\n  },\n  mounted() {\n    fetch(`${this.serverURL}/login`)\n      .then(response => response.json())\n      .then(json => (this.loginInfo = json))\n  },\n  \n\n\n  \n}\n\n\n\n\n// import { ref, onMounted } from 'vue'\n\n//       const serverInfo = ref({})\n//       const loginInfo = ref({})\n//       const serverURL = process.env.VUE_APP_SERVER_URL\n      \n \n//   onMounted(async () => {\n//     const serverData = await fetch(`${serverURL}/application`)\n//     serverInfo.value = await serverData.json()\n//     const loginData = await fetch(`${this.serverURL}/login`)\n//     loginInfo.value = await loginData.json()\n//   })\n\n\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n  .navbar-nav li button {\n    background: transparent;\n    border: none;\n    line-height: 20px;\n    font-size: 18px;\n    color: white;\n    padding: 25px 15px;\n  }\n\n  .btn-primary:active, .btn-primary.active, .open > .dropdown-toggle.btn-primary {\n    background-color: #e7e7e7;\n  }\n\n  button span.caret {\n    margin-left: 2px;\n  }\n\n  .plus-logo {\n    font-size: 10rem;\n    margin-left: -100px;\n    margin-right: -13px;\n  }\n\n  .hero-logo {\n    width: 161px;\n    margin-right: -161px;\n    margin-bottom: 88px;\n  }\n\n  .footer {\n    font-size: 0.9em;\n  }\n</style>\n"]}]}